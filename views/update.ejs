<%- include("partials/nav.ejs")%>

<form class="form-control m-auto w-25 mt-5"  action="/update/<%=user._id%>" method="post">
<input class="form-control mb-3" name="username"  type="username" value="<%=user.username%>" placeholder="enter username"> <br>
<input class="form-control mb-3" name="email" type="email" value="<%=user.email%>"  placeholder="enter email"> <br>
<input class="form-control mb-3" name="password" type="password" value="<%=user.password%>" disabled   placeholder="create password"> <br>

<button class="btn btn-primary">update user</button>



</form>
